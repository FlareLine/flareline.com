{
	"compilerOptions": {
		"esModuleInterop": true,
		"jsx": "react",
		"module": "CommonJS",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"rootDir": "./src",
		"sourceMap": true,
		"strictNullChecks": true,
		"target": "ES6"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
